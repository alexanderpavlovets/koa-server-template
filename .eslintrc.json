{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "comma-dangle": "off",
        "no-await-in-loop": "off",
        "max-len": ["error", { "code": 120 }]
    }
}
